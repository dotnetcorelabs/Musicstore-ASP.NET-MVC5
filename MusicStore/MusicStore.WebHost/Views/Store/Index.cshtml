@model IEnumerable<Genre>

@{
    ViewBag.Title = "Store";
}

<h3>Browse Genres</h3>

<p>
    select from @Model.Count() genres:
</p>

<ul class="list-group list-group-flush">
    @foreach (var genre in Model)
    {
        <li class="list-group-item">
            <a asp-action="Browse" asp-route-genreName="@genre.Name">@genre.Name</a>
        </li>
    }
</ul>